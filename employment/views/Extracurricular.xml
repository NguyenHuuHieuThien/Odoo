<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="mylib_extracurricular_act" model="ir.actions.act_window">
            <field name="name">Ngoại Khóa</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">mylib.extracurricular</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Thêm mới một khóa đào tạo
                </p>
            </field>
        </record>
        <record id="mylib_extracurricular_tree" model="ir.ui.view">
            <field name="name">Ngoại Khóa</field>
            <field name="model">mylib.extracurricular</field>
            <field name="arch" type="xml">
                <tree>

                    <field name="name"/>
                    <field name="time_happend"/>
                    <field name="state"/>
                    <field name="address"/>
                    <field name="person"/>

                </tree>
            </field>
        </record>
    </data>
</odoo>